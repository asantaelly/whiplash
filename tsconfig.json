{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-native",
    "baseUrl": "./",
    "paths": {
      "assets/*": ["assets/*"],
      "utils/*": ["src/utils/*"],
      "types/*": ["src/types/*"],
      "styles/*": ["src/styles/*"],
      "themes/*": ["src/themes/*"],
      "layouts/*": ["src/layouts/*"],
      "screens/*": ["src/screens/*"],
      "components/*": ["src/components/*"],
      "navigation/*": ["src/navigation/*"],
      "providers/*": ["src/providers/*"],
    },
    "types": ["node"]
  }
}
